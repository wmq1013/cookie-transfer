{
  "manifest_version": 3,
  "name": "Cookie 传输工具",
  "version": "1.0",
  "description": "在不同域名间传输 Cookie",
  "permissions": [
    "cookies",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}